    <!-- activity进入与退出 start -->
    <style name="bqmm_horizontal_slide" parent="android:Theme.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/AnimFade2</item>
    </style>

    <style name="AnimFade2" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/bqmm_slide_in_from_right</item>
        <item name="android:activityOpenExitAnimation">@anim/bqmm_slide_out_to_left</item>
        <item name="android:activityCloseExitAnimation">@anim/bqmm_slide_out_to_right</item>
        <item name="android:activityCloseEnterAnimation">@anim/bqmm_slide_in_from_left</item>
    </style>

