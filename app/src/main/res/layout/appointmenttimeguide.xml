<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/ll_appointmenttimeguide_main"
    android:background="#99000000"
     >
    <ImageView 
        android:id="@+id/iv_appointmenttimeguide_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_timeguid_top"
        android:layout_marginTop="5dip"
        android:layout_centerHorizontal="true"
        />
    <ImageView 
        android:id="@+id/iv_appointmenttimeguide_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_timeguide_bottom"
        android:layout_centerHorizontal="true"
        />

</RelativeLayout>
