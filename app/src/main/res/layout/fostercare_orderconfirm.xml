                        <TextView
                            android:id="@+id/tv_fostercareorderconfirm_bathcouponprice_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dip"
                            android:gravity="bottom"
                            android:text="已优惠￥0.00"
                            android:textColor="@color/a888888"
                            android:textSize="14sp"
                            android:visibility="visible" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/iv_fostercareorderconfirm_bath_two"
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_close"
                        android:scaleType="fitXY" />

                    <View
                        android:layout_width="wrap_content"
                        android:layout_height="0.8dip"
                        android:layout_alignParentBottom="true"
                        android:background="@color/bggray" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_fostercareorderconfirm_bringfood"
                    android:layout_width="match_parent"
                    android:layout_height="50dip" 
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/tv_fostercareorderconfirm_bringfood"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="宠粮"
                        android:textColor="@color/a222222"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_gravity="bottom"
                        android:layout_toLeftOf="@+id/iv_fostercareorderconfirm_bringfood"
                        android:gravity="bottom"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_fostercareorderconfirm_totalfoodfee"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="bottom"
                            android:text="￥0"
                            android:textColor="@color/orange"
                            android:textSize="18sp" 
                            android:visibility="gone"/>

                        <TextView
                            android:id="@+id/tv_fostercareorderconfirm_foodprice"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dip"
                            android:layout_marginRight="5dip"
                            android:gravity="bottom"
                            android:text="自带或到店购买"
                            android:textColor="@color/a888888"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/iv_fostercareorderconfirm_bringfood"
                        android:layout_width="20dip"
                        android:layout_height="20dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dip"
                        android:background="@drawable/icon_pay_normal"
                        android:scaleType="fitXY" 
                        android:visibility="invisible"/>
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dip"
                android:background="@color/white"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/ll_fostercareorderconfirm_pickup"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dip"
                        android:layout_marginLeft="10dip" 
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tv_fostercareorderconfirm_pickup_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:text="接送"
                            android:textColor="@color/a222222"
                            android:textSize="18sp" />

                        <ImageView
                            android:id="@+id/iv_fostercareorderconfirm_pickup_hint"
                            android:layout_width="30dip"
                            android:layout_height="30dip"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="3dip"
                            android:layout_toRightOf="@+id/tv_fostercareorderconfirm_pickup_title"
                            android:background="@drawable/icon_hint"
                            android:scaleType="fitXY" />

                        <TextView
                            android:id="@+id/tv_fostercareorderconfirm_pickup_hint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dip"
                            android:layout_marginRight="30dip"
                            android:layout_toRightOf="@+id/iv_fostercareorderconfirm_pickup_hint"
                            android:gravity="right"
                            android:text="接送名额已满"
                            android:textColor="@color/a666666"
                            android:textSize="16sp"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tv_fostercareorderconfirm_pickup"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="5dip"
                            android:layout_toLeftOf="@+id/iv_fostercareorderconfirm_pickup"
                            android:ellipsize="end"
                            android:gravity="right"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="@color/orange"
                            android:textSize="16sp" />

                        <ImageView
                            android:id="@+id/iv_fostercareorderconfirm_pickup"
                            android:layout_width="20dip"
                            android:layout_height="20dip"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dip"
                            android:background="@drawable/icon_pay_normal"
                            android:scaleType="fitXY" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.8dip"
                        android:layout_marginLeft="10dip"
                        android:background="@color/bggray"
                        android:visibility="gone" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/rl_fostercareorderconfirm_coupon"
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:paddingLeft="10dip" >

                    <TextView
                        android:id="@+id/tv_fostercareorderconfirm_coupon_pre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="优惠券"
                        android:textColor="@color/a333333"
                        android:textSize="17sp" />

                    <TextView
                        android:id="@+id/tv_fostercareorderconfirm_coupon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/iv_fostercareorderconfirm_conpon_rightarrow"
                        android:layout_toRightOf="@+id/tv_fostercareorderconfirm_coupon_pre"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLength="25"
                        android:singleLine="true"
                        android:text="无可用优惠券"
                        android:textColor="@color/aBB996C"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/iv_fostercareorderconfirm_conpon_rightarrow"
                        android:layout_width="30dip"
                        android:layout_height="30dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/icon_rightarrow"
                        android:scaleType="fitXY" />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" 
                    android:background="@color/bggray"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        >

                        <CheckBox
                            android:id="@+id/cb_fostercareappointment_agree"
                            style="@style/CustomCheckboxTheme"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:checked="true"
                            android:paddingLeft="10dip"
                            android:layout_marginLeft="10dp"
                            android:text="同意"
                            android:textColor="@color/a888888"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/tv_fostercareappointment_agreement"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/cb_fostercareappointment_agree"
                            android:text="《宠物家寄养协议》"
                            android:textColor="@color/aBB996C"
                            android:textSize="16sp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout 
                    android:id="@+id/layout_notice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="10dp"
                    android:background="@color/bggray"
                    >
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/wheel_val_1"
        android:orientation="horizontal"
        android:paddingLeft="10dip" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_gravity="center"
            android:gravity="center_vertical"
            android:layout_weight="2" >

            <TextView
                android:id="@+id/tv_fostercareorderconfirm_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="实付款：￥0"
                android:textColor="@color/aD1494F"
                android:textSize="16sp"
                />

        </RelativeLayout>

        <Button
            android:id="@+id/bt_fostercareorderconfirm_pay"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_weight="3"
            android:background="@color/aBB996C"
            android:text="去付款"
            android:textColor="@color/white"
            android:textSize="18sp" />
    </LinearLayout>

</LinearLayout>